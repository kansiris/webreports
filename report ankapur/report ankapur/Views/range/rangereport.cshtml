@if (Model != null)
{
    <p class="my-2 col-xl-12">
        Revenue : &nbsp;<b>Rs.@ViewBag.revenue </b>  &nbsp;  Amount Paid : &nbsp;<b>Rs.@ViewBag.Pricesum </b>  &nbsp;  Total Orders :<b> &nbsp;@ViewBag.modelCust </b> &nbsp; Start Date : &nbsp;<b>@ViewBag.selectdate1 </b>  &nbsp;-&nbsp;  End Date : &nbsp;<b>
            @ViewBag.selectdate</b> &nbsp;  Restaurant :<b>&nbsp; @ViewBag.restcode1 &nbsp; </b> 
       </p>
    <table class="table table-striped table-bordered table-hover" id="dataTable" width="100%" cellspacing="0">
        <thead>
            <tr>
                <th>Order ID</th>
                <th>Order Date</th>
                <th>Restaurant Code</th>
                @*<th>Totalamount</th>
                    <th>Orderstatus</th>*@
                <th>Customer Phone</th>
                @*<th>statusdiscription</th>*@
                <th>First name</th>
                <th>Last name</th>
                @*<th>Billing_Address</th>
                    <th>Delivery_Addresss</th>
                    <th>empcode</th>*@
                <th>Quantity</th>
                <th>Total Price</th>
                <th>Delivery Charges</th>
                <th>Cgst Charges</th>
                <th>Sgst Charges</th>
                <th>Discount</th>
                <th>Tip</th>
                <th>Amount Paid</th>
                <th>Remarks</th>
                <th>Order Type</th>
                <th>Transaction Id</th>
                <th>Transaction Status</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
                {
                <tr>
                    <td>@item.OrderId</td>
                    <td>@item.OrderDate</td>
                    <td>@item.restcode</td>
                    <td>@item.customerphone</td>
                    <td>@item.customerfname</td>
                    <td>@item.customerlname</td>
                    @*<td>@item.Totalorder</td>*@
                    @*<td>@item.Orderstatus</td>*@
                    <td>@item.Quantity</td>
                    <td>@item.TotalPrice</td>
                    <td>@item.Deliverycharges</td>
                    <td>@item.cgstcharges</td>
                    <td>@item.sgstcharges</td>
                    <td>@item.Discount</td>
                    <td>@item.Tip</td>
                    <td>@item.amountPaid</td>
                    <td>@item.Remarks</td>
                    <td>@item.OrderType</td>
                    <td>@item.TransactionId</td>
                    <td>@item.TransactionStatus</td>
                </tr>
            }

        </tbody>

    </table>

}

